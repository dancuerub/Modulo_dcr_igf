<odoo>
    <data noupdate="1">
        <!-- Secuencia para Ventas -->
        <record id="seq_venta" model="ir.sequence">
            <field name="name">Secuencia de Ventas</field>
            <field name="code">venta.model</field>
            <field name="prefix">VENTA</field>
            <field name="padding">5</field>
        </record>

        <!-- Secuencia para Facturas -->
        <record id="seq_factura" model="ir.sequence">
            <field name="name">Secuencia de Facturas</field>
            <field name="code">factura.model</field>
            <field name="prefix">FAC</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
